<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>study</servlet-name>
        <servlet-class>za.ac.tut.web.study</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signin</servlet-name>
        <servlet-class>za.ac.tut.web.signin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>za.ac.tut.web.signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Papers</servlet-name>
        <servlet-class>za.ac.tut.web.Papers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Notes</servlet-name>
        <servlet-class>za.ac.tut.web.Notes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Video</servlet-name>
        <servlet-class>za.ac.tut.web.Video</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>za.ac.tut.web.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Report</servlet-name>
        <servlet-class>za.ac.tut.web.Report</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>study</servlet-name>
        <url-pattern>/study.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signin</servlet-name>
        <url-pattern>/signin.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Papers</servlet-name>
        <url-pattern>/Papers.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Notes</servlet-name>
        <url-pattern>/Notes.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Video</servlet-name>
        <url-pattern>/Video.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Report</servlet-name>
        <url-pattern>/Report.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
